{
  "name": "active_admin",
  "version": "2.1.0",
  "description": "The administration framework for Ruby on Rails.",
  "main": "app/assets/javascripts/active_admin.js",
  "files": [
    "app/assets/images/*",
    "app/assets/javascripts/*.js",
    "app/assets/stylesheets/*.scss",
    "src/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "activeadmin/activeadmin"
  },
  "keywords": [
    "administration",
    "rails"
  ],
  "author": "David Rodr√≠guez <deivid.rodriguez@riseup.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/activeadmin/activeadmin/issues"
  },
  "homepage": "htts://activeadmin.info",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "common-tags": "^1.8.0",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "sass": "^1.69.0"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build:css": "sass app/assets/stylesheets/active_admin/_base.scss:app/assets/builds/active_admin.css app/assets/stylesheets/active_admin/print.scss:app/assets/builds/active_admin/print.css --load-path=app/assets/stylesheets --load-path=vendor/assets/stylesheets",
    "build:all": "npm run build && npm run build:css",
    "prepublishOnly": "rm -rf src && cp -R app/javascript/active_admin src"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "jquery-ujs": "^1.2.2"
  }
}
